<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.smartdatainc.fudo" >

    
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="com.google.android.providers.gsf.permission.READ_GSERVICES" />
      <!--
         The ACCESS_COARSE/FINE_LOCATION permissions are not required to use
           Google Maps Android API v2, but are recommended.
      -->
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission
        android:name="com.smartdatainc.fudo.permission.MAPS_RECEIVE"
        android:protectionLevel="signature" />
    
    

    <application
        android:name="com.smartdatainc.activities.MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >


        <activity
            android:name="com.smartdatainc.activities.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.smartdatainc.activities.AppActivity"
            android:clearTaskOnLaunch="true"
            android:launchMode="singleTop"
            android:screenOrientation="portrait">
        </activity>
        
        

        <activity android:name="com.smartdatainc.activities.DashboardActivity"></activity>
        <activity android:name="com.smartdatainc.activities.DishListActivity"></activity>
        <activity android:name="com.smartdatainc.activities.HotelListActivity"></activity>
        <activity android:name="com.smartdatainc.activities.TableActivity"></activity>
        <activity android:name="com.smartdatainc.activities.CartItemListActivity"></activity>
        <activity android:name="com.smartdatainc.activities.SearchOrderActivity"></activity>
        
            
        <activity
            android:name="com.smartdatainc.activities.MapsActivity"
            android:label="@string/title_activity_maps" >
        </activity>
        
        
        
        <activity android:name="com.smartdatainc.activities.SwipeRefreshActivity" />

        
        
        
        
        
           
                
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />
            <meta-data
            android:name="com.google.android.maps.v2.API_KEY"
            android:value="@string/google_maps_key" />
        
        
        
        
        
        <activity android:name="com.smartdatainc.activities.RecyclerActivity" />
        
        <meta-data
            android:name="io.fabric.ApiKey"
            android:value="29c1a652f01f26144dab196033434c65e43cebfb" />
       

        

            
       
       
       
      
        
       
       
         
        
 </application>
</manifest>
